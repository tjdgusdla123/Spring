<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="member">
  <!-- 로그인 처리를 위한 sql -->
  <select id="login" parameterType="gmail.ggangpae1.oracleserver.domain.Member"
  resultType="gmail.ggangpae1.oracleserver.domain.Member">
  
  select *
  from member
  where id=#{id} and pw=#{pw}
  </select>
  </mapper>